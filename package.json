{
    "name": "jax-portfolio",
    "version": "1.0.0",
    "description": "she do a build",
    "author": "Jax Engel",
    "license": "MIT",
    "main": "index.html",
    "scripts": {
        "autoprefixer": "postcss -u autoprefixer -r css/*.css",
        "scss": "sass scss/output.scss css/style.css",
        "serve": "browser-sync start --server --files \"css/*.css\"",
        "build:css": "npm run scss && npm run autoprefixer",
        "build": "npm run build:css",
        "watch:css": "onchange \"scss\" -- npm run build:css",
        "watch": "npm-run-all -p serve watch:css",
        "start": "npm run build && npm run watch"
    },
    "dependencies": {
        "autoprefixer": "^9.7.4",
        "browser-sync": "^2.26.7",
        "npm-run-all": "^4.1.5",
        "onchange": "^6.1.0",
        "sass": "^1.25.0"
    }
}
